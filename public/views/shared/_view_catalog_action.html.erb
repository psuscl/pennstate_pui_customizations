<% if record['json'].has_key?('user_defined') %>
  <% if record['json']['user_defined']['string_1'] %>
    <% url = "https://catalog.libraries.psu.edu/catalog/#{record['json']['user_defined']['string_1']}" %>
    <%= link_to raw('<i class="fa fa-university"></i> ' + t('actions.view_in_catalog')), url,
               :class => 'btn btn-default page_action view-catalog',
               :title => t('actions.view_in_catalog'),
               :target => "_blank"
    %>
  <% end %>
<% end %>
